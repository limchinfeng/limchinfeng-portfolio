{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@assets/*": ["public/assets/*"],
      "@data/*": ["src/data/*"],
      "@/*": ["src/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}